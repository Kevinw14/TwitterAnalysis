             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TwitterAnalysis"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Button x:Name="ANDButton" Content="AND" HorizontalAlignment="Left" Margin="165,108,0,0" VerticalAlignment="Top" Width="50" Click="ANDButton_Click"/>
        <Button x:Name="ORButton" Content="OR" HorizontalAlignment="Left" Margin="215,108,0,0" VerticalAlignment="Top" Width="50" Click="ORButton_Click" Background="#FFB5B5B5"/>

    </Grid>
