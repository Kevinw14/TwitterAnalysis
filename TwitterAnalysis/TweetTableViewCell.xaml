<local:TableViewCell x:Class="TwitterAnalysis.TweetTableViewCell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TwitterAnalysis"
             mc:Ignorable="d" Background="white">
    <StackPanel Orientation="Horizontal">
        <Label>Image</Label>
        <StackPanel Orientation="Vertical">
            <Label x:Name="UsernameLabel">Username</Label>
            <TextBlock x:Name="TweetLabel" TextWrapping="WrapWithOverflow">Tweet</TextBlock>
        </StackPanel>
    </StackPanel>
</local:TableViewCell>
